<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.test.healthapp2.MainActivity">

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="34dp"
        android:layout_height="34dp"
        android:layout_marginLeft="21dp"
        android:layout_marginStart="19dp"
        android:onClick="weight"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton4"
        app:layout_constraintLeft_toRightOf="@+id/imageButton4"
        app:layout_constraintTop_toTopOf="@+id/imageButton4"
        app:srcCompat="@android:drawable/btn_dialog"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="34dp"
        android:layout_height="34dp"
        android:layout_marginLeft="21dp"
        android:layout_marginStart="19dp"
        android:onClick="height"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton2"
        app:layout_constraintLeft_toRightOf="@+id/imageButton2"
        app:layout_constraintTop_toTopOf="@+id/imageButton2"
        app:srcCompat="@android:drawable/btn_star_big_on"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="34dp"
        android:layout_height="34dp"
        android:layout_marginLeft="79dp"
        android:layout_marginTop="343dp"
        android:onClick="heartRate"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/btn_star_big_on"
        android:layout_marginStart="77dp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="34dp"
        android:layout_height="34dp"
        android:layout_marginLeft="21dp"
        android:onClick="SPO2"
        app:layout_constraintLeft_toRightOf="@+id/imageButton"
        app:layout_constraintTop_toTopOf="@+id/imageButton"
        app:srcCompat="@android:drawable/btn_dialog"
        android:layout_marginStart="19dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton"
        app:layout_constraintVertical_bias="0.0" />

</android.support.constraint.ConstraintLayout>
